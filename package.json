{
  "name": "@bndynet/recharts-wrapper",
  "version": "1.1.0",
  "description": "A Recharts wrapper for easiler to code",
  "main": "dist/recharts-wrapper.umd.js",
  "module": "dist/recharts-wrapper.es5.js",
  "typings": "dist/types/recharts-wrapper.d.ts",
  "files": [
    "dist"
  ],
  "author": "Bendy Zhang <zb@bndy.net>",
  "repository": {
    "type": "git",
    "url": "https://github.com/bndynet/recharts-wrapper.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && npm run docs",
    "docs": "typedoc --theme ./node_modules/@bndynet/typedoc-default-themes/bin/default --out docs --target es6 --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "keywords": [
    "react",
    "reactjs",
    "chart",
    "react-component",
    "recharts"
  ],
  "devDependencies": {
    "@bndynet/header-injection-webpack-plugin": "^4.0.2",
    "@bndynet/typedoc-default-themes": "^0.5.1",
    "@commitlint/cli": "^7.1.2",
    "@semantic-release/git": "^7.0.8",
    "@types/classnames": "^2.2.7",
    "@types/d3-shape": "^1.2.7",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.11.0",
    "@types/randomcolor": "^0.5.1",
    "@types/react": "^16.7.18",
    "@types/recharts": "file:typings/recharts",
    "@types/recharts-scale": "^1.0.0",
    "acorn": "^6.0.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "colors": "^1.3.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^8.1.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.15.3",
    "prompt": "^1.0.0",
    "randomcolor": "^0.5.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "recharts": "^1.4.2",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "semantic-release": "^15.9.16",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^22.0.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "recharts": "^1.4.2",
    "randomcolor": "0.5.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
